Host *
  ServerAliveInterval 60
  # Share connections
  ControlMaster auto
  ControlPath ~/.ssh/%r@%h:%p
  # Keep the control open for 1 hour after all the connections exit
  # This also puts the initial connection into the background!
  ControlPersist 1h
  ServerAliveCountMax 30
  ServerAliveInterval 5
  # Not supported on Mac
  # ControlPersist 4h

Host aws-2015
     HostName ec2-54-172-83-212.compute-1.amazonaws.com
     User ec2-user

Host aws aws-2016
     HostName ec2-52-6-217-70.compute-1.amazonaws.com
     User ec2-user

Host qia* ril* mi3c* qin* research-gpu* myst*
  User mra9161

Host dewey*
  User dewey
  StrictHostKeyChecking no

Host tactic* mirmaid
  User apache

Host feynman* crick* rcfcluster-ftp
  User mra9161
  ForwardX11 yes

Host feynman crick
  Hostname feynman7

